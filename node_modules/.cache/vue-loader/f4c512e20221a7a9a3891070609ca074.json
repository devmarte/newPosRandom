infiniteScroll=\"true\" centerMode class=\"workSlide\">\n                <slide id=\"look\">\n                    <div class=\"glass\"></div>\n                    <h2>LOOK360</h2>\n                    <p>LOOK360 포털입니다.(등의 소개 및 설명)</p>\n                </slide>\n                <slide id=\"campus\">\n                    <div class=\"glass\"></div>\n                    <h2>CAMPUSTOWN</h2>\n                    <p>CAMPUSTOWN 사이트입니다.(등의 소개 및 설명)</p>\n                </slide>\n                <slide id=\"skate\">\n                    <div class=\"glass\"></div>\n                    <h2>SK8SPOT</h2>\n                    <p>SK8SPOT 사이트입니다.(등의 소개 및 설명)</p>\n                </slide>\n                <hooper-navigation slot=\"hooper-addons\"></hooper-navigation>\n                <hooper-pagination slot=\"hooper-addons\"></hooper-pagination>\n            </hooper>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {\n    Hooper,\n    Slide,\n    Navigation as HooperNavigation,\n    Pagination as HooperPagination,\n} from \"hooper\";\nimport \"hooper/dist/hooper.css\";\nexport default {\n    components: {\n        Hooper,\n        Slide,\n        HooperNavigation,\n        HooperPagination,\n    },\n    data() {\n        return {\n            title: \"최고가 아닌 <span>유일</span>이</br > 되려고 합니다.\",\n            works: [\n                {\n                    name: \"LOOK360\",\n                    desc: \"LOOK360 포털입니다.(등의 소개 및 설명)\",\n                },\n                {\n                    name: \"CAMPUSTOWN\",\n                    desc: \"CAMPUSTOWN 사이트입니다.(등의 소개 및 설명)\",\n                },\n                {\n                    name: \"SK8SPOT\",\n                    desc: \"SK8SPOT 사이트입니다.(등의 소개 및 설명)\",\n                },\n            ],\n        };\n    },\n};\n</script>\n\n<style lang=\"scss\">\n#works {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n\n    .container {\n        .workSlide {\n            height: calc(100vh - 10px - 85px - 300px);\n            max-height: 550px;\n            min-height: 200px;\n\n            #look {\n                background-image: url(\"../assets/look360page.jpg\");\n            }\n            #campus {\n                background-image: url(\"../assets/campuspage.jpg\");\n            }\n            #skate {\n                background-image: url(\"../assets/sk8spotpage.jpg\");\n            }\n            .hooper-slide {\n                position: relative;\n                display: flex;\n                flex-direction: column;\n                justify-content: center;\n                align-items: center;\n                border-radius: 8px;\n                cursor: pointer;\n                background-position: center;\n                background-repeat: no-repeat;\n                background-size: cover;\n\n                .glass {\n                    position: absolute;\n                    top: 0;\n                    left: 0;\n                    width: 100%;\n                    height: 100%;\n                    background: #fff;\n                    transition: 1s all;\n                    opacity: 0;\n                }\n                .motion-rise {\n                    transform: translateY(0);\n                    opacity: 1;\n                }\n                h2 {\n                    margin-bottom: 16px;\n                    background: #fff;\n                    padding: 12px 18px;\n                    border-radius: 8px;\n                    transform: translateY(60%);\n                    transition: 1s all;\n                    transition-delay: 0.8s;\n                    opacity: 0;\n                    box-shadow: 2px 2px 4px #ccc;\n                    z-index: 10;\n                }\n                p {\n                    background: #fff;\n                    padding: 12px 18px;\n                    color: #2b2b2b;\n                    border-radius: 8px;\n                    transform: translateY(60%);\n                    transition: 1s all;\n                    transition-delay: 0.8s;\n                    opacity: 0;\n                    box-shadow: 2px 2px 4px #ccc;\n                    z-index: 10;\n                }\n            }\n            .hooper-slide.is-active {\n                .glass {\n                    opacity: 0.6;\n                }\n                h2 {\n                    transform: translateY(0);\n                    opacity: 1;\n                }\n                p {\n                    transform: translateY(0);\n                    opacity: 1;\n                }\n            }\n        }\n    }\n}\n</style>"]}]}